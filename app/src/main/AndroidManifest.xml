<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pt">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GameActivityResult"></activity>
        <activity android:name=".GameActivitySwipe" />
        <activity android:name=".GameActivityShake" />
        <activity android:name=".ManagerActivity" />
        <activity android:name=".ManualActivity" />
        <activity android:name=".TimeActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".InputActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>